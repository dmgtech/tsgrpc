{
  "name": "protoc-gen-wsgrpc",
  "version": "1.0.0",
  "main": "dist/protoc-gen-wsgrpc.js",
  "types": "dist/protoc-gen-wsgrpc.ts",
  "license": "MIT",
  "scripts": {
    "build-ts": "tsc -b",
    "build": "tsc -b && nexe --target 10.16.2 --output dist/bin/protoc-gen-wsgrpc",
