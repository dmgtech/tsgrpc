{
  "name": "protoc-gen-tsgrpc",
  "version": "1.0.0",
  "main": "dist/protoc-gen-tsgrpc.js",
  "types": "dist/protoc-gen-tsgrpc.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b",
    "clean": "find test -type f -name '*.gen.*' -delete",
    "protoc-test": "grpc_tools_node_protoc --plugin=protoc-gen-tsgrpc=./cli.js -Itest --tsgrpc_out=./test test/example.proto",
    "protoc-test-json": "grpc_tools_node_protoc --plugin=protoc-gen-tsgrpc=./cli.js -Itest --tsgrpc_out=json:./test test/example.proto",
    "test:debug": "node --inspect-brk -r ../.pnp.js $(yarn bin jest) --runInBand",
    "test": "jest --coverage",
    "postinstall": "node postinstall.js"
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@types/node": "^12",
    "jest": "latest",
    "ts-jest": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "assert-never": "^1.2.1",
    "change-case": "^4.1.1",
    "grpc-tools": "^1.9.1",
    "protobuf-codec-ts": "workspace:protobuf-codec-ts",
    "protoc-plugin": "workspace:protoc-plugin"
  },
  "bin": {
    "protoc-gen-tsgrpc": "./cli.js",
    "protoc-tsgrpc": "./protoc.js"
  }
}
