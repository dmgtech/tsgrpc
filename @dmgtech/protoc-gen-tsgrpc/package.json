{
  "name": "@dmgtech/protoc-gen-tsgrpc",
  "version": "1.0.0",
  "main": "dist/protoc-gen-tsgrpc.js",
  "types": "dist/protoc-gen-tsgrpc.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "!dist/.tsbuildinfo",
    "cli.js",
    "postinstall.js",
    "protoc.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf test/generated/!(surrogates.ts|.gitignore)",
    "gen:bootstrap": "grpc_tools_node_protoc --plugin=protoc-gen-tsgrpc=./cli.js -Iprotoc-proto --tsgrpc_out=./src protoc-proto/google/protobuf/descriptor.proto protoc-proto/google/protobuf/compiler/plugin.proto",
    "gen:test": "grpc_tools_node_protoc --plugin=protoc-gen-tsgrpc=./cli.js -Itest/proto --tsgrpc_out=./test/generated test/proto/example.proto",
    "gen:json": "grpc_tools_node_protoc --plugin=protoc-gen-tsgrpc=./cli.js -Itest/proto --tsgrpc_out=json:./test/generated test/proto/example.proto",
    "test:debug": "node --inspect-brk -r ../../.pnp.js $(yarn bin jest) --runInBand",
    "test": "jest --coverage",
    "postinstall": "node postinstall.js",
    "prepublish": "tsc -b"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^12",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@dmgtech/tsgrpc-protobuf-codec": "workspace:@dmgtech/tsgrpc-protobuf-codec",
    "assert-never": "^1.2.1",
    "change-case": "^4.1.1",
    "grpc-tools": "^1.9.1"
  },
  "bin": {
    "protoc-gen-tsgrpc": "./cli.js",
    "protoc-tsgrpc": "./protoc.js"
  }
}
